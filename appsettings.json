{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HackerNewsInfo": {
    "prependUrl": "https://hacker-news.firebaseio.com/v0/",
    "newstoriesUrl": "https://hacker-news.firebaseio.com/v0/newstories.json",
    "maxitemUrl": "https://hacker-news.firebaseio.com/v0/maxitem.json",
    "singelStoryUrl": "https://hacker-news.firebaseio.com/v0/item/",
    "initialStoryFetchAmount": "50",
    "initialCacheTimeSpanInSeconds": "60"
  },
  "AllowedHosts": "*"
}
